#!/bin/bash

gs=10

make benchmark

echo "Benchmarking $gs rounds..."
for i in `seq $gs` ; do
	score[$i]="$(mean `./p8 -p "9H 7H 7S QS AS 4S" -h "9C 7C 6C 4C KC KH 3H" -h "8H 8C 3C KS" -m66 | awk '{ print $6 }' | xargs` | sed 's/\..*//')"
	echo -n "$i "
done
echo -e "\nFinal score: `mean ${score[*]} | sed 's/\..*//'`"
